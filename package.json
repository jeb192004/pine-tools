{
  "name": "pine-assist",
  "version": "1.0.3",
  "description": "An app made to assist with cli commands",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -l --arm64"
  },
  "repository": "https://github.com/jeb192004/pine-tools",
  "keywords": [
    "Electron",
    "pine64",
    "pinephone",
    "commands"
  ],
  "author": {
    "name": "James Burnison",
    "email": "jeb192004@gmail.com"
  },
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^17.0.0",
    "electron-builder": "^23.6.0"
  },
  "build": {
    "appId": "com.jeb.pine-command-assist",
    "productName": "Pine Command Assist",
    "linux": {
      "icon": "./icons/",
      "target": ["pacman","deb"]
    },
    "extraResources": [
      "res",
      "icons",
      "48x48.png"
    ],
    "directories": {
      "buildResources": "icons",
      "output": "output"
    }
  }
}
